@InProceedings{Schonfeld1990,
  Title                    = {Analysis and modeling of thermal-blooming compensation},
  Author                   = {Schonfeld, Jonathan F.},
  Booktitle                = {{Proc. SPIE}},
  Year                     = {1990},

  Address                  = {Bellingham, WA},
  Note                     = {Atmospheric Laser Beam Propagation;Atmospheric Thermal Blooming;Adaptive Thermal Blooming Compensation;Adaptive Laser Optics Control Systems;},
  Pages                    = {118 - 131},
  Volume                   = {1221},

  Abstract                 = {This paper surveys work of the last several years at Lincoln Laboratory in analysis and computer modeling of adaptive optics hardware, especially in the context of compensation for thermal blooming. We discuss an analytical theory of thermal blooming phase-compensation instability (PCI) incorporating the actuator geometry of real deformable mirrors and wavefront sensors; we also discuss novel algorithms for computer simulation of adaptive optics hardware. Our analytical formalism makes possible a quantitative analysis of the effects of the adaptive-optics control system on PCI, and also leads to a universality theorem for PCI growth rates under certain control-system assumptions. It also leads to the recognition that wind has a stronger effect on PCI growth rates than previously thought. Simulation algorithms discussed here include models of a Hartmann wavefront sensor, multidither adaptive optics, and a null-seeking servo. Analysis and algorithms are illustrated with output from MOLLY, a computer simulation of time-dependent adaptively-compensated laser propagation through turbulence and thermal blooming, optimized for the Cray-2 supercomputer. Of particular note is a comparison between MOLLY output and data from a laboratory study of phase-compensation instability.},
  Copyright                = {Compilation and indexing terms, Copyright 2005 Elsevier Engineering Information, Inc.},
  ISBN                     = {0-8194-0262-1},
  ISSN                     = {0277-786X},
  Journal                  = {Proceedings of SPIE - The International Society for Optical Engineering},
  Key                      = {Laser Beams -- Propagation},
