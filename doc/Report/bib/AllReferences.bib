  groups    = {Atmospheric Spectroscopy and CO2},
  keywords  = {Cavity ring down spectroscopy; Water; H2 O; HITRAN; Atmospheric window; 1.5 lm},
  owner     = {NWillers},
  timestamp = {2008.05.08},
  url       = {www.elsevier.com/locate/jms},
}

