@Article{Macko04,
  author    = {P. Macko and D. Romanini and S. N. Mikhailenko and O. V. Naumenko and S. Kassi and A. Jenouvrier and V. G. Tyuterev and A. Camparguea},
  title     = {{High sensitivity CW-cavity ring down spectroscopy of water in the region of the 1.5 um atmospheric window}},
  journal   = {J. Mol. Spectrosc.},
  year      = {2004},
  volume    = {227},
  number    = {2004},
  pages     = {90-108},
  abstract  = {The absorption spectrum of natural water vapour around 1.5 lm has been recorded with a typical sensitivity of 5 {\^A} 10{\`A}10 cm{\`A}1 by using a CW-cavity ring down spectroscopy set up based on fibred DFB lasers. A series of 31 DFB lasers has allowed a full coverage of the 6130.8---6748.5 cm{\`A}1 (1.63---1.48 lm) region corresponding to the H transparency band of the atmosphere. The line parameters (wavenumber and intensity) of a total of 5190 lines, including 4247 lines of water vapor, were derived by a one by one fit of the lines to a Voigt profile. Different isotopologues of water (H2 16 O, H2 18 O, H2 17 O, and HD16 O) present in natural abundance in the sample contribute to the spectrum. For the main isotopologue, H2 16 O, 2130 lines were measured with line intensities as weak as 10{\`A}29 cm/ molecule while only 926 lines (including a proportion of 30% inaccurate calculated lines) with a minimum intensity of 3 {\^A} 10{\`A}27 cm/ molecule are provided by the HITRAN and GEISA databases. Our comparison in the whole 5750---7965 cm{\`A}1 region, has also evidenced that an error in the process of conversion of the intensity units from cm{\`A}2 /atm to cm{\`A}1 /(molecule {\^A} cm{\`A}2 ) at 296 K, has led to H2 16 O line intensities values listed in the HITRAN-2000 database, systematically 8 % below the original FTS values. The rovibrational assignment was performed on the basis of the ab initio calculations by Schwenke and Partridge with a subsequent refinement and validation using the Ritz combination principle together with all previously measured water transitions relevant to this study. This procedure allowed determining 172, 139, 71, and 115 new energy levels for the H2 16 O, H2 18 O, H2 17 O, and HD16 O isotopologues, respectively. The results are compared with the available databases and discussed in regard of previous investigations by Fourier transform spectroscopy. The spectrum analysis has showed that most of the transitions which cannot be assigned to water are very weak and are due to impurities such as carbon dioxide and ammonia, leaving only about 3% of the observed transitions unassigned. The interest of a detailed knowledge of water absorption for trace detectors developed in the 1.5 lm range is underlined: for instance HDO contributes significantly to the considered spectrum while no HDO line parameters are provided by the HITRAN database. {\'O} 2004 Elsevier Inc. All rights reserved.},
  doi       = {10.1016/j.jms.2004.05.020},
