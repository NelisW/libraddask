@TechReport{Christo1999,
  Title                    = {{Thermochemistry and Kinetics Models for Magnesium/Teflon/ Viton Pyrotechnic Compositions}},
  Author                   = {F. C. Christo},
  Institution              = {DSTO, Aeronautical and Maritime Research Laboratory},
  Year                     = {1999},

