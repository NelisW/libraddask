@PhdThesis{Oezkan2005,
  author   = {B\"{u}lent \"{O}zkan},
  title    = {{Dynamic Modeling, Guidance, and Control of Homing Missiles}},
  school   = {{Middle East Technical University}},
  year     = {2005},
  abstract = {The dynamic modeling, guidance, and control of a missile with two relatively rotating parts connected to each other by means of a roller bearing. The governing differential equations of motion of the mentioned missile are derived. The aerodynamic model of the missile is constructed by means of the Missile Datcom software. The necessary transfer functions are then determined based on the equations of motion of the missile. The Linear Homing Guidance and the Parabolic Homing Guidance laws are introduced as alternatives to the Proportional Navigation Guidance law. The roll, pitch and yaw autopilots are designed using the determined transfer functions. As the roll autopilot is constructed to regulate the roll angle of the front body of the missile which is the controlled part, the pitch and yaw autopilots are designed to realize the command signals generated by the guidance laws. The guidance commands are in lateral acceleration components or the flight path angles of the missile. The proposed guidance laws are compared with the Proportional Navigation Guidance law.},
}

